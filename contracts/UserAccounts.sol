pragma solidity^0.4.0;

contract Accounts{
  
  mapping ( string  => string)  AccMap;
  
  function get_secret(string username) view returns(string){

    return (AccMap[username]);
  }
    
    
  function set(string username,string secret) public{
      
    AccMap[username]=secret;
  }

}

